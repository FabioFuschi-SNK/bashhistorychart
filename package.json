{
  "name": "BashHistoryChart",
  "version": "1.0.0",
  "description": "Bash History Chart",
  "private": true,
  "scripts": {
    "start": "gulp"
  },
  "dependencies": {
    "autoprefixer": "^9.7.4",
    "browser-sync": "^2.26.7",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-if": "^3.0.0",
    "gulp-inject": "^5.0.5",
    "gulp-less": "^4.0.1",
    "gulp-load-plugins": "^2.0.2",
    "gulp-notify": "^3.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-zetzer": "^1.0.4",
    "less-plugin-glob": "^3.0.0",
    "node-notifier": "^8.0.1",
    "pump": "^3.0.0",
    "yargs": "^15.1.0"
  },
  "frontendconfg": {
    "cssprecompiler": "less",
    "dir": {
      "js": "../js/",
      "css": "../css/"
    },
    "assets": {
      "js": ["main.js"],
      "css": [
        "style.css"
      ]
    },
    "global": {
      "title": "Bash History Chart"
    }
  },
  "browsersyncconfig": {
    "server": {
      "baseDir": "./build/"
    },
    "ui": {
      "port": 3001,
      "weinre": {
        "port": 3002
      }
    },
    "https": false,
    "notify": false,
    "online": false,
    "logLevel": "warn",
    "logFileChanges": false,
    "open": false,
    "ghostMode": false,
    "files": [
      "./build/**/*"
    ]
  },
  "autoprefixerconfig": {
    "cascade": false
  },
  "pathconfig": {
    "sourcedir": "./src",
    "targetdir": "./build"
  }
}
